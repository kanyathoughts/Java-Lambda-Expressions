package demo;

class Employee {
	String ename;
	int salary, experience;
	
	public Employee(String name, int sal, int exp) {
		this.ename = name;
		this.salary = sal;
		this.experience = exp;
	}
	
}

public class PredicateDemo2 {

	public static void main(String[] args) {
		
		Employee e1 = new Employee("Kanya", 35000, 2);
		
		

	}

}
