package demo;

@FunctionalInterface
interface Cab {
	public void cab();
}

class Ola implements Cab {

	@Override
	public void cab() {
		System.out.println("Ola cab booked...");
		
	}
	
}

public class Demo1 {
	

	public static void main(String[] args) {
		Ola o1 = new Ola();
		o1.cab();
		
		Cab o2 = new Ola();
		o2.cab();
		

	}

}
